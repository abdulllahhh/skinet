          "country",
          "line1",
          "postalCode",
          "state"
        ],
        "type": "object",
        "properties": {
          "line1": {
            "minLength": 1,
            "type": "string"
          },
          "line2": {
            "type": "string",
            "nullable": true
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "state": {
            "minLength": 1,
            "type": "string"
          },
          "postalCode": {
            "minLength": 1,
